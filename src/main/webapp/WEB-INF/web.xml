         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

   <servlet-mapping>
    <servlet-name>JsonbDemo</servlet-name>
    <url-pattern>/JsonbDemo</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>JsonbDemo</servlet-name>
    <servlet-class>com.oracle.jsonbdemo.JsonbDemoServlet</servlet-class>
  </servlet>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

</web-app>
